package main
import ("fmt")
func main(){
    var finalFee float32
    var a float32=2
    var b float32=100
    //Write your program logic here
    //Populate the variable: finalFee
    var mark float32=70
    var scholarship float32
    scholarship=mark/a
    var scholarship_percent float32
    scholarship_percent=scholarship/b
    var course_fee float32=25000
    var fees=course_fee*scholarship_percent
    var deducted_fees float32
    deducted_fees=course_fee-fees
    finalFee=deducted_fees+1500
     //Do not modify the below print statement for verification to work
     fmt.Println(finalFee)
}
