#PF-Exer-11

def display(num):
    message=""
    #write your logic here
    if((num%3==0)and(num%5==0)):
        message="zoom"
    elif(num%3==0):
        message="zip"
    elif(num%5==0):
        message="zap"
    else:
        message="Invalid"
    return message

#Provide different values for num and test your program
message=display(20)
print(message)
